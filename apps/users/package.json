  "scripts": {
    "dev": "vite",
    "prebuild": "cd assets && bun run build",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "format": "prettier --write . --ignore-path .prettierignore",
    "format:check": "prettier --check . --ignore-path .prettierignore",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "asset": "bun add file:../k-pay-assets",
    "setup": "node scripts/setup.js",
    "welcome": "node scripts/welcome.js",
    "setup:manual": "bun run setup:submodule && bun run setup:assets",
    "setup:submodule": "git submodule update --init --recursive || echo 'Warning: Could not initialize submodules. Make sure you have git access.'",
    "setup:assets": "cd assets && bun install && bun run build || echo 'Warning: Could not setup assets. Please check the assets directory.'"
  },
